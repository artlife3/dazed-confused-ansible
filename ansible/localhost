[webservers]
192.168.33.10

[webservers:vars]
templates_path=../templates
files_path=../files

permission=ugo+rw
permission_file=0666
permission_dir=0777
permission=ugo+rw
time_zone=Tokyo/Asia
log_dir=/var/log

tomact_dir=/opt/tomcat
tomcat_user=vagrant
tomcat_group=vagrant

elastic_conf_dir=/etc/elasticsearch
elastic_user=elasticsearch
elastic_group=vagrant

owner=vagrant
group=vagrant

# Httpd
apache_user=vagrant
apache_group=vagrant
httpd_conf_dir=/etc/httpd
httpd_document_root=/var/www/html

tools_path=/opt/tools
rbenv_path=/opt

# MySQL
mysq_user=mysql
mysq_group=vagrant
mysql_root_passwd=root

mycolor=$'32m'

